{
  "name": "@kikiutils/node",
  "version": "4.1.0",
  "description": "A utility library offering easy-to-use HTTP request wrappers, secure hash functions, flexible logging, datetime utilities, and more to simplify Node.js development.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/kikiutils-node.git"
  },
  "scripts": {
    "build": "rollup -c --configPlugin typescript=\"{ tsconfig: './tsconfig.rollup.json' }\"",
    "bumplog": "changelogen --bump"
  },
  "dependencies": {
    "@noble/hashes": "^1.4.0",
    "axios": "^1.7.2",
    "consola": "^3.2.3",
    "date-fns": "^3.6.0",
    "decimal.js": "^10.4.3",
    "millify": "^6.1.0",
    "pino": "^9.3.1",
    "pino-pretty": "^11.2.1"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.11",
    "changelogen": "^0.5.5",
    "glob": "^11.0.0",
    "hono": "^4.5.1",
    "rollup": "^4.19.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^7.1.2"
  },
  "keywords": [
    "axios",
    "consola",
    "crypto",
    "datetime",
    "enum",
    "environment",
    "hash",
    "hono",
    "http",
    "logging",
    "math",
    "node",
    "pino",
    "string",
    "utilities"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./*": {
      "import": "./*.mjs",
      "require": "./*.cjs",
      "types": "./*.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
