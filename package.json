{
  "name": "@kikiutils/node",
  "version": "5.1.0",
  "description": "A utility library offering easy-to-use HTTP request wrappers, secure hash functions, flexible logging, datetime utilities, and more to simplify Node.js development.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/kikiutils-node.git"
  },
  "scripts": {
    "build": "ts-project-builder './src/**/*.ts' --clean",
    "bumplog": "changelogen --bump",
    "release": "pnpm run build && pnpm run test && changelogen --push --release && sh ./build-and-publish.sh",
    "test": "cross-env TZ=UTC jest --coverage"
  },
  "devDependencies": {
    "@kikiutils/changelogen": "^0.7.0",
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.2",
    "@noble/hashes": "^1.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.6",
    "axios": "^1.7.7",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.4.3",
    "hono": "^4.6.8",
    "jest": "^29.7.0",
    "millify": "^6.1.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "ts-jest": "^29.2.5",
    "ts-project-builder": "3.3.1"
  },
  "keywords": [
    "axios",
    "consola",
    "crypto",
    "datetime",
    "enum",
    "environment",
    "hash",
    "hono",
    "http",
    "logging",
    "math",
    "node",
    "pino",
    "string",
    "utilities"
  ],
  "engines": {
    "node": ">=18.12.1"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": "./*.mjs",
      "require": "./*.cjs",
      "types": "./*.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
